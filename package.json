{
  "name": "vue3-colorpicker",
  "version": "2.0.6",
  "main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "author": "aesoper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aesoper101/vue3-colorpicker/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/aesoper101/vue3-colorpicker#readme",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && vite build --config ./vite.config.lib.ts",
    "serve": "vite preview",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "pub": "cd dist && npm publish --registry=https://registry.npmjs.org --access public && cd ..",
    "unpub": "npm unpublish vue3-colorpicker --force --registry=https://registry.npmjs.org",
    "prelink": "cd dist && yarn link && cd .. && yarn link vue3-colorpicker",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "rimraf storybook-static && build-storybook",
    "reinstall": "rimraf node_modules && yarn install"
  },
  "dependencies": {
    "@aesoper/normal-utils": "^0.1.5",
    "@popperjs/core": "^2.11.6",
    "@vueuse/core": "^9.5.0",
    "gradient-parser": "^1.0.2",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.4.2",
    "vue": "^3.2.44",
    "vue-types": "^4.2.1",
    "vue3-angle": "^0.1.6",
    "vue3-normal-library": "^0.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/vue3": "^6.5.13",
    "@types/gradient-parser": "^0.1.2",
    "@types/jest": "^29.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/compiler-sfc": "^3.2.44",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-loader": "^9.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.27.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "storybook-builder-vite": "^0.1.5",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-no-unsupported-browser-features": "^6.0.1",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.0.9"
  }
}
